    <navigation-rule>
        <from-view-id>/edit/editCompany.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>events</from-outcome>
            <to-view-id>events.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>manage-companies</from-outcome>
            <to-view-id>/manage/manageCompanies.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>registration</from-outcome>
            <to-view-id>registration.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>registration.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>login.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/composite/createCompany.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>manage-companies</from-outcome>
            <to-view-id>/manage/manageCompanies.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/manage/manageCompanies.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>manage-companies</from-outcome>
