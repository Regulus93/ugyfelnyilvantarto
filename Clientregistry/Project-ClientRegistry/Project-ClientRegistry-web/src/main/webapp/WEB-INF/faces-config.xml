    <navigation-rule>
        <from-view-id>/single-view/company.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>projects</from-outcome>
            <to-view-id>/view/company.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>events</from-outcome>
            <to-view-id>/view/company.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>contact-persons</from-outcome>
