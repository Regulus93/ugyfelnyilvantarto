        </navigation-case>
        <navigation-case>
            <from-outcome>view-contact-person</from-outcome>
            <to-view-id>/view/contact-person.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>create-project</from-outcome>
            <to-view-id>/composite/createProject.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>create-event</from-outcome>
            <to-view-id>/composite/createEvent.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>add-contact-person</from-outcome>
            <to-view-id>/composite/addContactPerson.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/composite/createEvent.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>manage-events</from-outcome>
            <to-view-id>/manage/manageEvents.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>create-event</from-outcome>
            <to-view-id>/composite/createEvent.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
